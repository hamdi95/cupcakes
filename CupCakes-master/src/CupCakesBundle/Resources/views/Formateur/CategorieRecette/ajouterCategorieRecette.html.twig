{% extends "::layoutf.html.twig" %}
{% block containt %}

    {{ form_start(form) }}
    <div class="col-lg-12 col-md-9">
        <div class="card">
            <div class="header">
                <h4 class="title">{{ titre }}</h4>
            </div>
            <div class="content">
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            {{ form_row(form.nomCatRec, {'label':"Nom Categorie Recette" ,'attr': {'class': 'form-control border-input'}}) }}
                            <p style="color: red">{{ message}}</p>
                        </div>
                    </div>
                </div>
                <div>
                    <input type="submit" class="btn btn-info btn-fill btn-wd" value="Enregistrer" />
                    {{ form_row(form._token) }}
                </div>
            </div>
        </div>
    </div>
    {{ form_end(form) }}
{% endblock %}
