{% extends "::layoutf.html.twig" %}
{% block containt %}

<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="header">
                        <h4 class="title">liste clients affectes  sessions</h4>

                    </div>
                    <div class="content table-responsive table-full-width">
                        <table class="table table-striped">
                            <thead>
                            <th>ID</th>
                            <th>id session</th>
                            <th>Etat educate</th>
                            <th>Date inscri</th>
                            <th>id Utilisateur</th>
                            <th>Supprimer</th>
                            <th>Modifier</th>
                            </thead>
                            <tbody>
                            {% for educ in educates  %}
                                <tr>
                                    <td>
                                        {{  educ.idSes.id }}
                                    </td>
                                    <td>
                                        {{  educ.etatEduc }}
                                    </td>
                                    <td>
                                        {{  educ.dateIscri |date('d-M-Y') }}
                                    </td>
                                    <td>
                                        {{ educ.idUser }}
                                    </td>
                                </tr>
                            {% endfor %}
                            </tbody>
                        </table>

                    </div>
                </div>
            </div>
            <script type="text/javascript"></script>



            {% endblock %}

            {% block javascript %}
            <script>

                {% if  message %}
                alert("{{ message }}");
                {% endif %}
            </script>
            {% endblock %}